# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

# mount_image	fstype	device					flags
/system_image	emmc	/dev/block/bootdevice/by-name/system	flags=slotselect;display="System_Image";backup=1;flashimg=1
/persist_image	emmc	/dev/block/bootdevice/by-name/persist	flags=display="Persist_Image";backup=1;flashimg=1

# mount		fstype	device					flags
/boot		emmc	/dev/block/bootdevice/by-name/boot	flags=slotselect;backup=1
/system_root	ext4	/dev/block/bootdevice/by-name/system	flags=slotselect;display="System";backup=1;flashimg=1
/data		ext4	/dev/block/bootdevice/by-name/userdata	flags=encryptable=footer;backup=1
/persist	ext4	/dev/block/bootdevice/by-name/persist	flags=display="Persist";backup=1
/splash		emmc	/dev/block/bootdevice/by-name/splash	flags=display="Splash Image";backup=1;flashimg=1
/misc		emmc	/dev/block/bootdevice/by-name/misc
/modem		emmc	/dev/block/bootdevice/by-name/modem	flags=slotselect;display="Modem";backup=1;flashimg=1
/firmware	vfat	/dev/block/bootdevice/by-name/modem	flags=display="Firmware";slotselect;backup=1;mounttodecrypt;fsflags=context=u:object_r:firmware_file:s0
/efs1		emmc	/dev/block/bootdevice/by-name/modemst1	flags=backup=1;display="EFS";flashimg=1
/efs2		emmc	/dev/block/bootdevice/by-name/modemst2	flags=backup=1;subpartitionof=/efs1;flashimg=1
/efsc		emmc	/dev/block/bootdevice/by-name/fsc	flags=backup=1;subpartitionof=/efs1;flashimg=1
/efsg		emmc	/dev/block/bootdevice/by-name/fsg	flags=backup=1;subpartitionof=/efs1;flashimg=1
